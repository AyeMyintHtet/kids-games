{
  "name": "Learny Land",
  "slug": "learnyLand",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./src/assets/images/icon.png",
  "scheme": "learnyLand",
  "userInterfaceStyle": "automatic",
  "newArchEnabled": true,
  "splash": {
    "image": "./src/assets/images/icon.png",
    "resizeMode": "contain",
    "backgroundColor": "#ffffff"
  },
  "assetBundlePatterns": [
    "**/*"
  ],
  "ios": {
    "supportsTablet": true,
    "bundleIdentifier": "com.ayemyinthtet.learnyland"
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./src/assets/images/icon.png",
      "backgroundColor": "#ffffff"
    },
    "package": "com.ayemyinthtet.learnyland"
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./src/assets/images/icon.png"
  },
  "plugins": [
    "expo-router",
    "expo-font",
    [
      "expo-av",
      {
        "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
      }
    ]
  ],
  "experiments": {
    "typedRoutes": true
  }
}